{
  "name": "schuler-maintenance-manager",
  "description": "Schuler maintenance manager",
  "version": "0.0.1",
  "author": "elunic AG",
  "license": "proprietary",
  "scripts": {
    "dev": "concurrently -n asset-backend,file-backend,user-backend,maintenance-backend,hub-backend,asset-frontend,maintenance-frontend,hub-frontend \"npm run dev:asset-backend\" \"npm run dev:file-backend\" \"npm run dev:user-backend\" \"npm run dev:maintenance-backend\" \"npm run dev:hub-backend\"  \"npm run dev:asset-frontend\" \"npm run dev:maintenance-frontend\" \"npm run dev:hub-frontend\" ",
    "dev:asset-backend": "cd asset-backend && npm run dev",
    "dev:file-backend": "cd file-backend && npm run dev",
    "dev:user-backend": "cd user-backend && npm run dev",
    "dev:maintenance-backend": "cd maintenance-backend && npm run dev",
    "dev:hub-backend": "cd hub-backend && npm run dev",
    "dev:asset-frontend": "cd asset-frontend && npm run dev",
    "dev:maintenance-frontend": "cd maintenance-frontend && npm run dev",
    "dev:hub-frontend": "cd hub-frontend && npm run dev",
    "ia": "npx npm-run-all -l -p -c --aggregate-output 'i:*'",
    "i:shared": "npm i",
    "i:asset-backend": "cd asset-backend/ && npm i",
    "i:file-backend": "cd file-backend/ && npm i",
    "i:user-backend": "cd user-backend/ && npm i",
    "i:maintenance-backend": "cd maintenance-backend/ && npm i",
    "i:hub-backend": "cd hub-backend/ && npm i",
    "i:asset-frontend": "cd asset-frontend/ && npm i",
    "i:maintenance-frontend": "cd maintenance-frontend/ && npm i",
    "i:hub-frontend": "cd hub-frontend/ && npm i",
    "check": "npm-run-all -l -p -c --aggregate-output 'check:*'",
    "check:shared": "cd asset-backend/ && npm run check",
    "check:asset-backend": "cd asset-backend/ && npm run check",
    "check:file-backend": "cd file-backend/ && npm run check",
    "check:user-backend": "cd user-backend/ && npm run check",
    "check:maintenance-backend": "cd maintenance-backend/ && npm run check",
    "check:hub-backend": "cd hub-backend/ && npm run check",
    "check:asset-frontend": "cd asset-frontend/ && npm run check",
    "check:maintenance-frontend": "cd maintenance-frontend/ && npm run check",
    "check:hub-frontend": "cd hub-frontend/ && npm run check",
    "fix": "npm-run-all -l -p -c --aggregate-output 'fix:*'",
    "fix:shared": "eslint -c shared/.eslintrc.fix.json --no-eslintrc --fix 'shared/**/*.{ts,js}'",
    "fix:asset-backend": "cd asset-backend/ && npm run fix",
    "fix:file-backend": "cd file-backend/ && npm run fix",
    "fix:user-backend": "cd user-backend/ && npm run fix",
    "fix:maintenance-backend": "cd maintenance-backend/ && npm run fix",
    "fix:hub-backend": "cd hub-backend/ && npm run fix",
    "fix:asset-frontend": "cd asset-frontend/ && npm run fix",
    "fix:maintenance-frontend": "cd maintenance-frontend/ && npm run fix",
    "fix:hub-frontend": "cd hub-frontend/ && npm run fix",
    "shell": "docker-compose -f docker-compose.shell.yml build && docker-compose -f docker-compose.shell.yml run --rm -u 1000 --name schuler-maintenance-manager --service-ports app bash || true && echo Stopping environment... && docker-compose -f docker-compose.shell.yml down",
    "shell:build": "docker-compose -f docker-compose.shell.yml build --no-cache",
    "shell:join": "docker exec -it schuler-maintenance-manager bash",
    "shell:stop": "docker-compose -f docker-compose.shell.yml down",
    "precommit:asset-backend": "cd asset-backend/ && npm run precommit",
    "precommit:file-backend": "cd file-backend/ && npm run precommit",
    "precommit:user-backend": "cd user-backend/ && npm run precommit",
    "precommit:maintenance-backend": "cd maintenance-backend/ && npm run precommit",
    "precommit:hub-backend": "cd hub-backend/ && npm run precommit",
    "precommit:asset-frontend": "cd asset-frontend/ && npm run precommit",
    "precommit:maintenance-frontend": "cd maintenance-frontend/ && npm run precommit",
    "precommit:hub-frontend": "cd hub-frontend/ && npm run precommit",
    "precommit": "npx npm-run-all -l -s -c 'precommit:*'"
  },
  "keywords": [],
  "dependencies": {
    "@elunic/is-iso-date": "^2.0.0",
    "@elunic/logger": "^4.0.4",
    "@elunic/logger-nestjs": "^1.2.0",
    "@nestjs/common": "^7.6.1",
    "@nestjs/core": "^7.6.1",
    "@nestjs/platform-express": "^7.6.1",
    "@nestjs/schedule": "^1.0.1",
    "@nestjs/swagger": "^4.7.6",
    "@nestjs/testing": "^7.6.1",
    "@nestjs/typeorm": "^7.1.5",
    "@nestjsx/crud": "^4.6.2",
    "@nestjsx/crud-typeorm": "^4.6.2",
    "bootstrap": "^4.6.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "common-errors": "^1.2.0",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "express-basic-auth": "^1.2.0",
    "fs-extra": "^9.0.0",
    "http-errors": "^1.8.0",
    "install": "^0.13.0",
    "joi": "^17.3.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "material-design-icons-iconfont": "^6.1.0",
    "module-alias": "^2.2.2",
    "moment": "^2.29.1",
    "mysql": "^2.18.1",
    "nestjs-joi": "^1.0.0",
    "node-cache": "^4.2.1",
    "npm": "^7.24.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.3",
    "source-map-support": "^0.5.19",
    "swagger-ui-express": "^4.1.5",
    "typeorm": "^0.2.29",
    "typeorm-naming-strategies": "^1.1.0",
    "url-join": "^4.0.1",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@elunic/ecs-commitlint": "^0.1.1",
    "@elunic/ecs-prettier": "^0.2.1",
    "@elunic/ecs-tsconfig": "^0.1.1",
    "@elunic/eslint-config-ecs": "^0.1.10",
    "@types/common-errors": "^1.0.1",
    "@types/concurrently": "^5.2.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "2.8.8",
    "@types/cron": "^1.7.3",
    "@types/express": "^4.17.9",
    "@types/fs-extra": "^8.1.0",
    "@types/http-errors": "^1.8.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.165",
    "@types/module-alias": "^2.0.0",
    "@types/morgan": "^1.9.2",
    "@types/mysql": "^2.15.16",
    "@types/node": "^12.12.47",
    "@types/prettier": "^2.1.5",
    "@types/source-map-support": "^0.5.3",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/url-join": "^4.0.0",
    "@types/uuid": "^7.0.4",
    "concurrently": "^5.2.0",
    "eslint": "^7.15.0",
    "husky": "^2.7.0",
    "lint-staged": "^10.5.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "typescript": "~4.1.0"
  }
}
